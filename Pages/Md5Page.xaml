<Page x:Class="development_kits.Pages.Md5Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Md5Page">
    <Grid Margin="20">
        <StackPanel>
            <Button Content="Back" Click="Back_Click" Width="80" />
            <TextBlock Text="MD5 Tool" FontSize="18" Margin="0,10" />
            <TextBox x:Name="InputText" Height="80" TextWrapping="Wrap" />
            <Button Content="Compute MD5" Click="Compute_Click" Margin="0,10" />
            <TextBox x:Name="OutputText" Height="60" IsReadOnly="True" />
        </StackPanel>
    </Grid>
</Page>